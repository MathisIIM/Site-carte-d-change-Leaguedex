        function toggleSortingDropdownDisplay(dropdown_to_display) {
            //console.log(window.getComputedStyle(document.getElementById(dropdown_to_display)).display);

            if(window.getComputedStyle(document.getElementById(dropdown_to_display)).display === 'none') {
                document.getElementById(dropdown_to_display).style.display = "block"; 
            } else {
                document.getElementById(dropdown_to_display).style.display = "none"; 
            }

            // all other dropdown displays need to be closed
            if(dropdown_to_display != 'month-options') {
                month_options_dropdown = document.getElementById('month-options');
                if(month_options_dropdown) {
                    month_options_dropdown.style.display = "none"; 
