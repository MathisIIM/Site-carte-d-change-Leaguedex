                    ).done(function() {

                        setTimeout(function () {
                            is_loading = false;
                        }, 1000);
                    });
            }

            $( document ).ready(function() {
                // handle scroll events to update content
                $(window).scroll(function() {
                    const scroll_pos = $(window).scrollTop();

                    //Load next page
                    if( is_loading === false
                        && ( scroll_pos >= ( $('#end_of_content').position().top - $(window).height() - 800 ) )
                        && scroll_pos < (  $('#end_of_content').position().top + 200 ) )
                    {
                        is_loading = true;
                        loadNextPage();
                    }
                });
            });
