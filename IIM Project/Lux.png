                !event.target.matches('#custom_width') && !event.target.matches('#custom_height') && 
                !event.target.matches('#resolution_button') && !event.target.matches('#resolution-options') &&
                !event.target.matches('#change_resolution_at_least') && !event.target.matches('#change_resolution_exactly') && 
                !event.target.matches('#month_button') && !event.target.matches('#year_button')
                
            
            
            ) {
                var dropdowns = document.getElementsByClassName("sorting-dropdown-content");
                var i;
                for (i = 0; i < dropdowns.length; i++) {
                    var dropdown_element = dropdowns[i];
                    if (window.getComputedStyle(dropdown_element).display === 'block') {

                        dropdown_element.style.display = 'none';


                    }
                }
            }
        }

